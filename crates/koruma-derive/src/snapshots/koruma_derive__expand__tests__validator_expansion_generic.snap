#[derive(koruma::bon::Builder)]
#[derive(Clone, Debug)]
pub struct GenericRangeValidation<T> {
    pub min: T,
    pub max: T,
    pub actual: Option<T>,
}
impl<T, S: generic_range_validation_builder::State> GenericRangeValidationBuilder<T, S>
where
    S::Actual: koruma::bon::IsUnset,
{
